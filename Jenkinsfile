pipeline {
     agent any
     stages {
             stage('INIT'){
	                 steps {
			      echo ' my name is akash and i am learning devops '
			 }
             }
	     stage{'BUILD'){
	                  steps {
			       echo "building sample project"
			  }
             }
	     stage('DEPLOY'){
	                   steps {
			        echo "deployed in staging area"
			   }

	     }
      }
}

